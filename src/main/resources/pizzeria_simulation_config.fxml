                              <Label text="Walk in customer">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font></Label>
                              <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="8.0">
                                 <children>
                                    <Label text="Mean:*">
                                       <font>
                                          <Font size="11.0" />
                                       </font></Label>
                                    <TextField fx:id="walkInCustomerMean" prefWidth="50.0" styleClass="input-text-field" text="15" HBox.hgrow="ALWAYS">
                                       <font>
                                          <Font size="10.0" />
                                       </font></TextField>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
