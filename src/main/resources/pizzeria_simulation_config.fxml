                           <children>
                              <Label styleClass="so-input-label" stylesheets="@style.css" text="Counter">
                                 <font>
                                    <Font name="System Bold" size="11.0" />
                                 </font></Label>
                              <HBox prefWidth="200.0">
                                 <children>
                                    <Label prefWidth="60.0" text="Mean:*">
                                       <font>
                                          <Font size="11.0" />
                                       </font></Label>
                                    <TextField fx:id="counterMean" prefWidth="50.0" styleClass="input-text-field" text="2" HBox.hgrow="ALWAYS">
                                       <font>
                                          <Font size="10.0" />
                                       </font></TextField>
                                 </children>
                              </HBox>
                              <HBox prefWidth="200.0">
                                 <children>
                                    <Label prefWidth="60.0" text="Variance:*">
                                       <font>
                                          <Font size="11.0" />
                                       </font></Label>
                                    <TextField fx:id="counterVariance" maxHeight="10.0" prefWidth="50.0" styleClass="input-text-field" text="0.5" HBox.hgrow="ALWAYS">
