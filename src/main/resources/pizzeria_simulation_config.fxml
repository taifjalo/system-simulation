      <VBox alignment="CENTER" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="alertLabel" alignment="CENTER" prefWidth="200.0" spacing="5.0" visible="false">
               <children>
                  <ImageView fitHeight="25.0" fitWidth="25.0">
