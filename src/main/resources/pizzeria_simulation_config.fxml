               <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Label text="Number of cooks:*">
                           <font>
                              <Font size="11.0" />
                           </font></Label>
                        <TextField fx:id="numberOfCooks" onAction="#handleCooksInput" prefWidth="50.0" styleClass="input-text-field" text="4">
                           <font>
                              <Font size="10.0" />
                           </font></TextField>
                     </children>
                  </HBox>
                  <ListView fx:id="cooks" prefWidth="200.0" styleClass="cooks-list-view">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></ListView>
                  <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Label text="Simulation time">
                           <font>
                              <Font size="11.0" />
                           </font>
                        </Label>
                        <TextField fx:id="simulationTime" prefWidth="50.0" styleClass="input-text-field" text="1000">
                           <font>
                              <Font size="10.0" />
                           </font>
                        </TextField>
