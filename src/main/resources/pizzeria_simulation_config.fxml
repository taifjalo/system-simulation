                              <HBox prefWidth="200.0">
                                 <children>
                                    <Label prefWidth="60.0" text="Variance:*">
                                       <font>
                                          <Font size="11.0" />
                                       </font></Label>
                                    <TextField fx:id="deliveryVariance" prefHeight="10.0" prefWidth="50.0" styleClass="input-text-field" text="5" HBox.hgrow="ALWAYS">
                                       <font>
                                          <Font size="10.0" />
                                       </font></TextField>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
